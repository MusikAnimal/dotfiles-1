[user]
	name = Ryan Sullivan
	email = ryan@ryansullivan.name
[color]
	ui = true
[alias]
	aa = add .
	br = branch
	ca = commit -a
	ci = commit
	co = checkout
	config-github = !bash ~/.dotfiles/bin/git-config-github
	config-remove-github = config --remove-section github
	depth = rev-parse --show-cdup
	df = diff
	dfa = !`git config diff.guitool` .
	pom = push origin master
	pr = pull --rebase
	root = !pwd
	st = status
	unstage = reset HEAD
[diff]
	guitool = meld
[instaweb]
	httpd = apache2
[core]
	excludesfile = ~/.dotfiles/gitignore
[merge]
	tool = meld
[mergetool]
	keepBackup = false
